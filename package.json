{
  "name": "react-portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://vinayakhegde.github.io/react-portfolio",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.2.0"
  },
  "scripts": {
    "clean": "rm -r build/*",
    "start": "react-scripts start",
    "build": "npm run clean && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "publish": "gh-pages -d build",
    "predeploy": "npm run build",
    "deploy": "npm run predeploy && npm run publish",
    "lint": "eslint --fix src",
    "prettify": "prettier --write \"src/**/*.+(js|jsx|json|css|scss|md)\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "classnames": "^2.2.6",
    "react-motion": "^0.5.2",
    "react-visibility-sensor": "^5.1.1",
    "gh-pages": "^2.1.1",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1"
  }
}
